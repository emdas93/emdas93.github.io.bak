{
	"name": "vuetify-project",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "npm run build:client && npm run build:server",
		"build:client": "vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/client",
		"build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
		"build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server",
		"generate": "rm -rf dist ; vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/client && npm run build:server && node prerender && node sitemap",
		"preview": "vite preview",
		"lint": "eslint . --fix --ignore-path .gitignore",
		"deploy": "gh-pages -d dist/static"
	},
	"dependencies": {
		"@mdi/font": "7.4.47",
		"@unhead/ssr": "^1.9.16",
		"@unhead/vue": "^1.9.16",
		"buffer": "^6.0.3",
		"commonmark": "^0.31.0",
		"core-js": "^3.37.1",
		"github-markdown-css": "^5.6.1",
		"gray-matter": "^4.0.3",
		"markdown-it": "^14.1.0",
		"markdown-it-anchor": "^9.0.1",
		"markdown-it-table-of-contents": "^0.6.0",
		"pinia": "^2.1.7",
		"roboto-fontface": "*",
		"vite-raw-plugin": "^1.0.2",
		"vue": "^3.4.31",
		"vue-meta": "^3.0.0-alpha.10",
		"vuetify": "^3.6.11"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.0.5",
		"eslint": "^8.57.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.4.0",
		"eslint-plugin-vue": "^9.27.0",
		"gh-pages": "^6.1.1",
		"sass": "1.77.6",
		"sitemap": "^8.0.0",
		"unplugin-fonts": "^1.1.1",
		"unplugin-vue-components": "^0.27.2",
		"unplugin-vue-router": "^0.10.0",
		"vite": "^5.3.3",
		"vite-plugin-node-polyfills": "^0.22.0",
		"vite-plugin-vuetify": "^2.0.3",
		"vue-router": "^4.4.0",
		"vuex": "^4.1.0"
	}
}
